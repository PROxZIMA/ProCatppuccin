  <weather_update_time value="108000" />
  <!-- The weather update time in seconds. This can only be used if you're using a custom weather key -->

  <location_update_mintime value="30" />
  <!-- The amount of time between two location updates (in minutes, must be an integer value) -->

  <location_update_mindistance value="10000" />
  <!-- The minimum distance (in meters) to get a location update -->

  <show_weather_updates value="false" />
  <!-- If false, t-ui won't show information about the weather in the output field -->


  <!-- #### The format used to show the unlock counter ### -->

  <unlock_counter_format value="Unlocked %c times (%a10/)%n%t(Unlock n. %i --> %w)3" />
  <!-- The format used to show the unlock counter -->

  <unlock_time_divider value="%n" />
  <!-- The divider between the last unlock times -->

  <unlock_time_order value="1" />
  <!-- 1 = up-down. 2 = down-up -->

  <unlock_counter_cycle_start value="6.00" />
  <!-- The starting hour of the unlock counter cycle (hh.mm) -->

  <not_available_text value="n/a" />
  <!-- The text shown when a value is not available -->

  <back_button_enabled value="true" />
  <!-- If true, the back button will put the previous command inside the input area -->

</BEHAVIOR>
